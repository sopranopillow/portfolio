{
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": "apps/vr-tests/**/*",
      "options": {
        "printWidth": 100
      }
    },
    {
      "files": ["*.html", "*.htm"],
      "options": {
        "trailingComma": "es5"
      }
    }
  ]
}
